  <link rel="stylesheet" href="/css/register.css" type="text/css">
  <title>Registrasi</title>
</head>

<body>
  <div id="login-box">
    <form class="registrasi-1" action="/user/register" method="post">
      <div class="registrasi">
        <h1>REGISTRATION</h1>
        <input type="text" placeholder="Nama Lengkap" name="nama">
        <input type="text" placeholder="Email" name="email">
        <input type="password" placeholder="Password" name="password">
        <input type="submit" value="register">
      </div>
      <div class="right-box">
        <span class="signwith">Register With<br>Sosial Media</span>
        <button class="sosial facebook">
          Register with Facebook
        </button>
        <button class="sosial google">
          Register with Google+
        </button>
      </div>
    </form>
  </div>


